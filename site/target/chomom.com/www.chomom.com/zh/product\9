		$(function(){
			$(window).scroll(function(){
				var flag="+";
				if ($(window).scrollTop() > 0) {
					var k2 = $(window).scrollTop()/2;
					k2<0?flag=" -":" +";
					$(".subpage_ad .bgimg.pcitem").css("background-position-y","calc(50% "+ flag + " " + Math.abs(k2) +"px)");
