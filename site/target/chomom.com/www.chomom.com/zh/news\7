<script>
    $(window).scroll(function(){
         if($(window).scrollTop()>0){
            $('#w_grid-1572666596036a').addClass('fixed')
            $('#w_fimg-1572666616553a').addClass('fixed')
            $('.tNav').addClass('fixed')
            $('.head_lan').addClass('fixed')
        }else{
            $('#w_grid-1572666596036a').removeClass('fixed')
            $('#w_fimg-1572666616553a').removeClass('fixed')
            $('.tNav').removeClass('fixed')
            $('.head_lan').removeClass('fixed')
      }
    })
    //触控吸顶
    $('#w_grid-1572666596036a').mouseenter(function(){
      $('#w_grid-1572666596036a').addClass('fixed')
      $('#w_fimg-1572666616553a').addClass('fixed')
      $('.tNav').addClass('fixed')
      $('.head_lan').addClass('fixed')
    }).mouseleave(function(){
        if($(window).scrollTop()==0){
          $('#w_grid-1572666596036').removeClass('fixed')
          $('#w_fimg-1572666616553').removeClass('fixed')
          $('.tNav').removeClass('fixed')
          $('.head_lan').removeClass('fixed')
        }
    });
</script>
